{
    "env": {
        "ndkPath": "/home/kodenamekrak/ndk/r26b"
    },
    "configurations": [
        {
            "defines": [
                "MOD_ID=\"PinkCore\"",
                "VERSION=\"0.1.0\"",
                "__GNUC__",
                "__aarch64__"
            ],
            "includePath": [
                "${workspaceFolder}/extern/includes/libil2cpp/il2cpp/libil2cpp",
                "${workspaceFolder}/extern/includes/questui/shared/cppcodec",
